Person person = new Person();
Console.WriteLine(person.Name);
person.Age = 20;
person.Print();
class Person
{
string name = "Kirill";
int age = 1;
public int Age
{
set { age = value; }
}
public string Name
{
get { return name; }
}

public void Print() => Console.WriteLine($"Name: {name} Age: {age}");
}
